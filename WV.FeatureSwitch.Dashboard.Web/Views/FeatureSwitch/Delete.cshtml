@model WV.FeatureSwitch.Dashboard.Web.ViewModels.FeatureViewModel

@{
    
}


<div>
    <h4>Delete Choosing Event</h4>
    <div class="alert alert-danger">Are you sure you want to remove this choosing party?</div>
    <dl class="dl-horizontal">
        <dt>
            <label asp-for="Id"></label>
        </dt>
        <dd>
            @Model.Id
        </dd>
        <dt>
            <label asp-for="Name"></label>
        </dt>
        <dd>
            @Model.Name
        </dd>
        <dt>
            <label asp-for="Flag"></label>
        </dt>
        <dd>
            @Model.Flag
        </dd>
      


    </dl>

    <form method="post">
        <div asp-validation-summary="All" class="text-danger"></div>
        <div class="control-label col-md-2"></div>
        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-danger" /> &nbsp;&nbsp;
            <a asp-action="Index">Back to List</a>
        </div>
    </form>
</div>
